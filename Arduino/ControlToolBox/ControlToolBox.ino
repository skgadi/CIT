int incomingByte;      // a variable to read incoming serial data intoint AI0P = 0;int Val0 = 0;int AnalogReadVal;int AO0P = 5;byte AORead[3] = {0,0,0};void setup() {  // initialize serial communication:  Serial.begin(115200);}void loop() {  // see if there's incoming serial data:  if (Serial.available() > 0) {    if (Serial.readBytes(AORead, 3)) {      if (AORead[0] == 0X31) {        analogWrite(AO0P, AORead[1]);            }      if (AORead[0] == 0X32) {        Val0 = analogRead(AI0P);        Serial.print("[");        Serial.print(Val0);        Serial.print("]E");      }    }  }}